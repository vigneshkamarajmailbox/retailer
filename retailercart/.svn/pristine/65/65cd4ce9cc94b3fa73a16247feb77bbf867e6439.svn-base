package com.botree.common.model;

public class WidgetModel {
    /** displayCode. */
    private String displayCode;
    /** displayValue. */
    private String displayValue;
    /** displayIcon. */
    private String displayIcon;
    /** displayColor. */
    private String displayColor;

    /**
     * @return the displayCode
     */
    public final String getDisplayCode() {
        return displayCode;
    }

    /**
     * @param displayCodeIn the displayCode to set
     */
    public final void setDisplayCode(final String displayCodeIn) {
        displayCode = displayCodeIn;
    }

    /**
     * @return the displayValue
     */
    public final String getDisplayValue() {
        return displayValue;
    }

    /**
     * @param displayValueIn the displayValue to set
     */
    public final void setDisplayValue(final String displayValueIn) {
        displayValue = displayValueIn;
    }

    /**
     * @return the displayIcon
     */
    public final String getDisplayIcon() {
        return displayIcon;
    }

    /**
     * @param displayIconIn the displayIcon to set
     */
    public final void setDisplayIcon(final String displayIconIn) {
        displayIcon = displayIconIn;
    }

    /**
     * @return the displayColor
     */
    public final String getDisplayColor() {
        return displayColor;
    }

    /**
     * @param displayColorIn the displayColor to set
     */
    public final void setDisplayColor(final String displayColorIn) {
        displayColor = displayColorIn;
    }
}
