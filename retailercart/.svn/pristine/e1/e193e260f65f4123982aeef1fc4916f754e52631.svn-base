package com.botree.interdbentity.model;

import java.io.Serializable;
import java.util.List;

/**
 * This class contains the transaction data from InterDB to other application.
 * @author vinodkumar.a
 */
public class TransactionModel implements Serializable {

    /** cmpCode. */
    private String cmpCode;

    /** distrCode. */
    private List<String> distrCode;

    /** interDBProcess. */
    private String interDBProcess;

    /** updateList. */
    private List updateList;

    /** entity. */
    private String entity;

    /** actionList. */
    private List<ActionApiEntity> actionList;

    /** customerCode. */
    private String customerCode;

    /** orderNo. */
    private String orderNo;

    /**
     * @return the cmpCode
     */
    public final String getCmpCode() {
        return cmpCode;
    }

    /**
     * @param cmpCodeIn the cmpCode to set
     */
    public final void setCmpCode(final String cmpCodeIn) {
        this.cmpCode = cmpCodeIn;
    }

    /**
     * @return the distrCode
     */
    public final List<String> getDistrCode() {
        return distrCode;
    }

    /**
     * @param distrCodeIn the distrCode to set
     */
    public final void setDistrCode(final List<String> distrCodeIn) {
        this.distrCode = distrCodeIn;
    }

    /**
     * @return the interDBProcess
     */
    public final String getInterDBProcess() {
        return interDBProcess;
    }

    /**
     * @param interDBProcessIn the interDBProcess to set
     */
    public final void setInterDBProcess(final String interDBProcessIn) {
        interDBProcess = interDBProcessIn;
    }

    /**
     * @return the updateList
     */
    public final List getUpdateList() {
        return updateList;
    }

    /**
     * @param updateListIn the updateList to set
     */
    public final void setUpdateList(final List updateListIn) {
        updateList = updateListIn;
    }

    /**
     * @return the entity
     */
    public final String getEntity() {
        return entity;
    }

    /**
     * @param entityIn the entity to set
     */
    public final void setEntity(final String entityIn) {
        entity = entityIn;
    }

    /**
     * @return the actionList
     */
    public final List<ActionApiEntity> getActionList() {
        return actionList;
    }

    /**
     * @param actionListIn the actionList to set
     */
    public final void setActionList(final List<ActionApiEntity> actionListIn) {
        actionList = actionListIn;
    }

    /**
     * @return the customerCode
     */
    public final String getCustomerCode() {
        return customerCode;
    }

    /**
     * @param customerCodeIn the customerCode to set
     */
    public final void setCustomerCode(final String customerCodeIn) {
        customerCode = customerCodeIn;
    }

    /**
     * @return the orderNo
     */
    public final String getOrderNo() {
        return orderNo;
    }

    /**
     * @param orderNoIn the orderNo to set
     */
    public final void setOrderNo(final String orderNoIn) {
        orderNo = orderNoIn;
    }
}
