package com.botree.retailerssfa.models;

import com.google.gson.annotations.SerializedName;

import static com.botree.retailerssfa.service.JSONConstants.TAG_CURRENT_MONTH_VAL;
import static com.botree.retailerssfa.service.JSONConstants.TAG_CURRENT_MONTH_VOL;
import static com.botree.retailerssfa.service.JSONConstants.TAG_WORK_DAYS;
import static com.botree.retailerssfa.service.JSONConstants.TAG_YEAR_VAL;
import static com.botree.retailerssfa.service.JSONConstants.TAG_YEAR_VOL;

/**
 * Created by shantarao on 26/3/18.
 */

public class TargetModel extends SalesmanModel {


    @SerializedName(TAG_CURRENT_MONTH_VOL)
    private Integer monthVolume=0;

    @SerializedName(TAG_CURRENT_MONTH_VAL)
    private Double monthValue=0.0;

    @SerializedName(TAG_YEAR_VOL)
    private Integer yearVolume=0;

    @SerializedName(TAG_YEAR_VAL)
    private Double yearValue=0.0;

    @SerializedName(TAG_WORK_DAYS)
    private Integer workingDays=0;

    public Integer getMonthVolume() {
        return monthVolume;
    }

    public void setMonthVolume(Integer monthVolume) {
        this.monthVolume = monthVolume;
    }

    public Double getMonthValue() {
        return monthValue;
    }

    public void setMonthValue(Double monthValue) {
        this.monthValue = monthValue;
    }

    public Integer getYearVolume() {
        return yearVolume;
    }

    public void setYearVolume(Integer yearVolume) {
        this.yearVolume = yearVolume;
    }

    public Double getYearValue() {
        return yearValue;
    }

    public void setYearValue(Double yearValue) {
        this.yearValue = yearValue;
    }

    public Integer getWorkingDays() {
        return workingDays;
    }

    public void setWorkingDays(Integer workingDays) {
        this.workingDays = workingDays;
    }
}
