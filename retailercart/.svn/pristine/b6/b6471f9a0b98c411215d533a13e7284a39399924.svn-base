package com.botree.retailerssfa.models;


import com.google.gson.annotations.SerializedName;

import static com.botree.retailerssfa.service.JSONConstants.TAG_CMP_CODE;
import static com.botree.retailerssfa.service.JSONConstants.TAG_GST_STATE_CODE;
import static com.botree.retailerssfa.service.JSONConstants.TAG_GST_STATE_NAME;
import static com.botree.retailerssfa.service.JSONConstants.TAG_GST_UNION_TERRITORY;

public class GSTModel {

    @SerializedName(TAG_CMP_CODE)
    private String cmpCode;

    private String distCode;

    private String salesmanCode;

    private String routeCode;

    private String customerCode;

    private String gstNo;

    @SerializedName(TAG_GST_STATE_CODE)
    private String gstStateCode;

    @SerializedName(TAG_GST_STATE_NAME)
    private String gstStateName;

    @SerializedName(TAG_GST_UNION_TERRITORY)
    private String unionTerritoryFlag;

    private String panNo;

    private String gstType;

    private String gstStatus;

    public String getGstStateCode() {
        return gstStateCode;
    }

    public void setGstStateCode(String gstStateCode) {
        this.gstStateCode = gstStateCode;
    }

    public String getGstStateName() {
        return gstStateName;
    }

    public void setGstStateName(String gstStateName) {
        this.gstStateName = gstStateName;
    }

    public String getUnionTerritoryFlag() {
        return unionTerritoryFlag;
    }

    public void setUnionTerritoryFlag(String unionTerritoryFlag) {
        this.unionTerritoryFlag = unionTerritoryFlag;
    }

    public String getDistCode() {
        return distCode;
    }

    public void setDistCode(String distCode) {
        this.distCode = distCode;
    }

    public String getSalesmanCode() {
        return salesmanCode;
    }

    public void setSalesmanCode(String salesmanCode) {
        this.salesmanCode = salesmanCode;
    }

    public String getRouteCode() {
        return routeCode;
    }

    public void setRouteCode(String routeCode) {
        this.routeCode = routeCode;
    }

    public String getCustomerCode() {
        return customerCode;
    }

    public void setCustomerCode(String customerCode) {
        this.customerCode = customerCode;
    }

    public String getGstNo() {
        return gstNo;
    }

    public void setGstNo(String gstNo) {
        this.gstNo = gstNo;
    }

    public String getPanNo() {
        return panNo;
    }

    public void setPanNo(String panNo) {
        this.panNo = panNo;
    }

    public String getGstType() {
        return gstType;
    }

    public void setGstType(String gstType) {
        this.gstType = gstType;
    }

    public String getGstStatus() {
        return gstStatus;
    }

    public void setGstStatus(String gstStatus) {
        this.gstStatus = gstStatus;
    }


    public String getCmpCode() {
        return cmpCode;
    }

    public void setCmpCode(String cmpCode) {
        this.cmpCode = cmpCode;
    }
}
