package com.botree.interdbentity.model;

/**
 * GST State Master Entity class.
 * @author vinodkumar.a
 */
public class GSTStateMasterEntity extends AbstractEntity {

    /** gstStateCode. */
    private String gstStateCode;

    /** gstStateName. */
    private String gstStateName;

    /** unionTerritoryFlag. */
    private String unionTerritoryFlag;

    /**
     * @return the gstStateCode
     */
    public final String getGstStateCode() {
        return gstStateCode;
    }

    /**
     * @param gstStateCodeIn the gstStateCode to set
     */
    public final void setGstStateCode(final String gstStateCodeIn) {
        gstStateCode = gstStateCodeIn;
    }

    /**
     * @return the gstStateName
     */
    public final String getGstStateName() {
        return gstStateName;
    }

    /**
     * @param gstStateNameIn the gstStateName to set
     */
    public final void setGstStateName(final String gstStateNameIn) {
        gstStateName = gstStateNameIn;
    }

    /**
     * @return the unionTerritoryFlag
     */
    public final String getUnionTerritoryFlag() {
        return unionTerritoryFlag;
    }

    /**
     * @param unionTerritoryFlagIn the unionTerritoryFlag to set
     */
    public final void setUnionTerritoryFlag(final String unionTerritoryFlagIn) {
        unionTerritoryFlag = unionTerritoryFlagIn;
    }
}
