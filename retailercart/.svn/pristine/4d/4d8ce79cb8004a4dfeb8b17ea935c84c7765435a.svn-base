<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <stroke android:width="0dip"
                android:color="#69B0FB" />
            <solid android:color="#69B0FB" />
            <corners
                android:topLeftRadius="120dp"
                android:topRightRadius="120dp"
                android:bottomLeftRadius="120dp"
                android:bottomRightRadius="120dp"
                />
            <gradient android:angle="180"
                android:endColor="#836CF9"
                android:startColor="#69B0FB"
                android:centerColor="#7395F9"/>
        </shape>
    </item>
</selector>