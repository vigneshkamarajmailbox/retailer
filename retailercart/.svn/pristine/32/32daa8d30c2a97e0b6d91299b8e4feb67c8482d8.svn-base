package com.botree.interdbentity.model;

import java.util.Date;

/**
 * Class contains the system notification related fields.
 * @author vinodkumar.a
 */
public class SystemNotificationEntity extends AbstractEntity {

    /** messageType. */
    private String messageType;

    /** validFrom. */
    private Date validFrom;

    /** validTo. */
    private Date validTo;

    /** messageQuery. */
    private String messageQuery;

    /** message. */
    private String message;

    /** isActive. */
    private String isActive;

    /** intervals. */
    private String intervals;

    /**
     * @return the messageType
     */
    public final String getMessageType() {
        return messageType;
    }

    /**
     * @param messageTypeIn the messageType to set
     */
    public final void setMessageType(final String messageTypeIn) {
        messageType = messageTypeIn;
    }

    /**
     * @return the validFrom
     */
    public final Date getValidFrom() {
        return validFrom;
    }

    /**
     * @param validFromIn the validFrom to set
     */
    public final void setValidFrom(final Date validFromIn) {
        validFrom = validFromIn;
    }

    /**
     * @return the validTo
     */
    public final Date getValidTo() {
        return validTo;
    }

    /**
     * @param validToIn the validTo to set
     */
    public final void setValidTo(final Date validToIn) {
        validTo = validToIn;
    }

    /**
     * @return the messageQuery
     */
    public final String getMessageQuery() {
        return messageQuery;
    }

    /**
     * @param messageQueryIn the messageQuery to set
     */
    public final void setMessageQuery(final String messageQueryIn) {
        messageQuery = messageQueryIn;
    }

    /**
     * @return the message
     */
    public final String getMessage() {
        return message;
    }

    /**
     * @param messageIn the message to set
     */
    public final void setMessage(final String messageIn) {
        message = messageIn;
    }

    /**
     * @return the isActive
     */
    public final String getIsActive() {
        return isActive;
    }

    /**
     * @param isActiveIn the isActive to set
     */
    public final void setIsActive(final String isActiveIn) {
        isActive = isActiveIn;
    }

    /**
     * @return the intervals
     */
    public final String getIntervals() {
        return intervals;
    }

    /**
     * @param intervalsIn the intervals to set
     */
    public final void setIntervals(final String intervalsIn) {
        intervals = intervalsIn;
    }
}
