package com.botree.interdbentity.model;

/**
 * LOB Master Entity class.
 * @author vinodkumar.a
 */
public class LOBMasterEntity extends AbstractEntity {

    /** lobCode. */
    private String lobCode;

    /** lobName. */
    private String lobName;

    /**
     * @return the lobCode
     */
    public final String getLobCode() {
        return lobCode;
    }

    /**
     * @param lobCodeIn the lobCode to set
     */
    public final void setLobCode(final String lobCodeIn) {
        lobCode = lobCodeIn;
    }

    /**
     * @return the lobName
     */
    public final String getLobName() {
        return lobName;
    }

    /**
     * @param lobNameIn the lobName to set
     */
    public final void setLobName(final String lobNameIn) {
        lobName = lobNameIn;
    }

}
