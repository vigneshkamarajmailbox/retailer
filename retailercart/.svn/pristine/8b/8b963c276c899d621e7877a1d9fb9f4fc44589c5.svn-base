/* page spinner */
.page.spinner::before {
  content: '';
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  left: 50%;
  width: 50px;
  height: 50px;
  margin-top: -10px;
  margin-left: -10px;
  border-radius: 50%;
  border: 4px solid #ffffff;
    border-top-color: rgb(255, 255, 255);
  border-top-color: #184379;
  animation: spinner .8s linear infinite;
}