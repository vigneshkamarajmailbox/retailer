/* Home page card Design */
/* new */
.p-0
{
  padding: 0 !important;
}
.p-4
{
  padding: 4px;
}
.wedget-card
{
  display: flex;
  height: auto;
  margin: 14px 8px;
  flex-wrap: wrap;
}
.wedget-card .box-design
{
    border-left: 8px solid darkblue;
    background: #f7f5f5;
}
.box-design
{
  margin: 2px 4px;
  margin-bottom: 8px;
  border-radius: 12px !important;
  width: 44%;
}
.box-content
{
  margin-bottom: 0;
  padding: 8px;
  display: flex;
  flex-wrap: wrap;
}
.icon-box
{
  background-color: #3d3d82;
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.53);
  height: 38px;
  width: 38px;
  line-height: 38px;
  text-align: center;
  border-radius: 50%;
  display: inline-block;
  margin-top: 8px;
}
.icon-box i
{
  font-size: 20px;
  margin: 8px;
  color: white;
}
.box-text
{
  padding: 6px;
}
.box-text .title
{
  font-size: 10px;
  margin-bottom: 4px;
}
.box-text .sub-title
{
  font-weight: bold;
}

@media screen and (min-width: 768px) {
  .box-design
  {
    width: 30%;
  }
  .box-content
  {
    padding: 24px 12px;
  }
  .box-text .title
  {
    font-size: 18px;
  }
  .box-text .sub-title
  {
    font-size: 20px;
  }
  .icon-box
  {
    margin-right: 8px;
  } 
  .icon-box i {
    font-size: 24px;
    margin: 7px;
    color: white;
  }
}


