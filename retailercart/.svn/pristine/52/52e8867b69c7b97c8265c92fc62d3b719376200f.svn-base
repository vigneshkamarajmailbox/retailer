package com.botree.interdbentity.model;

/**
 * Scheme Slab Entity class.
 * @author vinodkumar.a
 */
public class SchemeSlabEntity extends AbstractEntity {

    /** schemeCode. */
    private String schemeCode;

    /** slabNo. */
    private String slabNo;

    /** slabFrom. */
    private Double slabFrom;

    /** slabTo. */
    private Double slabTo;

    /** payout. */
    private Double payout;

    /** slabRange. */
    private String slabRange;

    /** forEvery. */
    private Double forEvery = 0d;

    /** uomCode. */
    private String uomCode;

    /**
     * @return the schemeCode
     */
    public final String getSchemeCode() {
        return schemeCode;
    }

    /**
     * @param schemeCodeIn the schemeCode to set
     */
    public final void setSchemeCode(final String schemeCodeIn) {
        this.schemeCode = schemeCodeIn;
    }

    /**
     * @return the slabNo
     */
    public final String getSlabNo() {
        return slabNo;
    }

    /**
     * @param slabNoIn the slabNo to set
     */
    public final void setSlabNo(final String slabNoIn) {
        this.slabNo = slabNoIn;
    }

    /**
     * @return the slabFrom
     */
    public final Double getSlabFrom() {
        return slabFrom;
    }

    /**
     * @param slabFromIn the slabFrom to set
     */
    public final void setSlabFrom(final Double slabFromIn) {
        this.slabFrom = slabFromIn;
    }

    /**
     * @return the slabTo
     */
    public final Double getSlabTo() {
        return slabTo;
    }

    /**
     * @param slabToIn the slabTo to set
     */
    public final void setSlabTo(final Double slabToIn) {
        this.slabTo = slabToIn;
    }

    /**
     * @return the payout
     */
    public final Double getPayout() {
        return payout;
    }

    /**
     * @param payoutIn the payout to set
     */
    public final void setPayout(final Double payoutIn) {
        this.payout = payoutIn;
    }

    /**
     * @return the slabRange
     */
    public final String getSlabRange() {
        return slabRange;
    }

    /**
     * @param slabRangeIn the slabRange to set
     */
    public final void setSlabRange(final String slabRangeIn) {
        this.slabRange = slabRangeIn;
    }

    /**
     * @return the forEvery
     */
    public final Double getForEvery() {
        return forEvery;
    }

    /**
     * @param forEveryIn the forEvery to set
     */
    public final void setForEvery(final Double forEveryIn) {
        this.forEvery = forEveryIn;
    }

    /**
     * @return the uomCOde
     */
    public final String getUomCode() {
        return uomCode;
    }

    /**
     * @param uomCodeIn the uomCOde to set
     */
    public final void setUomCode(final String uomCodeIn) {
        this.uomCode = uomCodeIn;
    }

